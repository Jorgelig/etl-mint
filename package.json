    "convict": "^5.0.0",
    "debug": "^4.1.1",
    "lodash": "^4.17.14",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.0",
    "punycode": "^2.1.1",
    "puppeteer": "^2.1.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
